<main class="main">
  <div class="inner">
    <div class="greeting">
        <% if user_signed_in? %>
          <%= "こんにちは、#{current_user.name}さん" %>
        <% end %>
    </div>
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
        <% @prototypes.each do |prototype| %>
          <%= render 'prototype', prototype: prototype %>
        <% end %>
    </div>
  </div>
</main>
